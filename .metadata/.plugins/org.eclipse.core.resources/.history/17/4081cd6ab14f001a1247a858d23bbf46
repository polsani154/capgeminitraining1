package practise;
enum SeatStatus
{
	Booked,
	Blocked,
	Available,
	None
}

public class Seat {

	public double price;
	public SeatStatus seatStatus;
	public Integer seatId;
	public Seat() {
		// TODO Auto-generated constructor stub
	}
	
	public void  seat(double price,SeatStatus status,Integer seatId)
	{
	
		this.price=price;
		seatStatus=status;
		this.seatId=seatId;
	}
}
