package practise;

class A
{
	int i;
	public A(int x) {
		// TODO Auto-generated constructor stub
	}
	public A()
	{
		System.out.println("hai");
	}
	void A(int x)
	{
		this.i=x;
	}
}
public class constructor extends A {

	int j;
	public constructor() {
		// TODO Auto-generated constructor stub
		super(5);
	}
	public constructor(int x,int y) {
		// TODO Auto-generated constructor stub
		super(x);
		this.j=y;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A a=new A();
		A b=new A();
		A c=a;
		
	}

}
