import java.util.*;
public class StreamsDemo {
	public StreamsDemo() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String []args)
	{
		ArrayList arrlist=new ArrayList();
		arrlist.add("sandeep");
		arrlist.add("Tharun");
		arrlist.add("Varun");
		arrlist.add("teja");
		arrlist.add("yaseen");
		arrlist.add("jagadish");
		arrlist.stream().filter(f->f.toString().endswith("n")).forEach(System.out::println);
	}

}
