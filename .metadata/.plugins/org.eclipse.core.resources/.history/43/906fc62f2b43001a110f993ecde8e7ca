
class S  extends Thread 
{
	public void run()
	{
		for(int i=0;i<5;i++)
		{
			if(i%2==0)
			{
				for (int j = 0; j < 6; j++) {
					try {
						Thread.sleep(100);
					} catch (InterruptedException e) {
						// TODO Auto-generated catch block
						e.printStackTrace();
					}
					System.out.print("#");
				}
			}
			if(i==1)
			{
				System.out.print("#     ");
			}
			if(i==3)
			{
				System.out.print("     #");
			}
		}
	}
}
class A
{
	public void run()
	{
		for(int i=0;i<5;i++)
		{
			if(i==0 || i==2)
			{
				for (int j = 0; j < 6; j++) {
					System.out.print("#");
				}
			}else
			{
				System.out.println("#    #");
			}
		}
	}
}
class N extends Thread
{
	public void run()
	{
		for (int i = 0; i < 5; i++) {
			System.out.print("#");
			for (int j = 1; j < i; j++) {
				System.out.print(" ");
			}
			if(i!=0)
			System.out.print("#");
			
			for (int j = i; j < 5; j++) {
				
			}
			System.out.print("#");
			System.out.println("");
		}
	}
}

public class sandeep extends Thread {

	public sandeep() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String []args)
	{
		N n=new N();
		n.start();
	}

}
