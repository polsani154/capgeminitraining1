package practise;
enum SeatStatus
{
	Booked,
	Blocked,
	Available,
	None
}
enum SeatType
{
	Seat,
	None
}

public class Seat {

	public double price;
	public SeatStatus seatStatus;
	public SeatType seatType;
	public Integer seatId;
	

	public void  seat(double price,SeatType type,Integer idd)
	{
		this.price=price;
		seatType=type;
		if(type==SeatType.Seat)
		{
			this.seatStatus=seatStatus.Available;
		}else
		{
			this.seatStatus=seatStatus.None;
		}
		this.seatId=seatId;
	}
}
