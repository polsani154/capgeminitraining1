
class S
{
	public void run()
	{
		for(int i=0;i<5;i++)
		{
			if(i%2==0)
			{
				System.out.println("#####");
			}
			if(i==1)
			{
				System.out.println("#    ");
			}
			if(i==3)
			{
				System.out.println("    #");
			}
		}
	}
}
