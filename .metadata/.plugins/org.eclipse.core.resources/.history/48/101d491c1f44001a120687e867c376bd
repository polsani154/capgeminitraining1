import java.util.ArrayList;
import java.util.Arrays;
import java.util.*;


public class exercise7 {

	public exercise7() {
		// TODO Auto-generated constructor stub
	}
	public static int reverse(Integer a)
	{
		int rev=0;
		while(a>0)
		{
			rev=(rev*10)+(a%10);
			a=a/10;
		}
		return rev;
		
	}
	public static int[] getSorted(Integer[] arr)
	{
		ArrayList arrlist=new ArrayList();
	for (int i = 0; i < arr.length; i++) {
//		arr[i]=reverse(arr[i]);
		arrlist.add(reverse(arr[i]));
	}	
		
		for (Object object : arrlist) {
			
		}
		Collections.sort(arrlist);
		Arrays.asList(arrlist);
		System.out.println(arrlist);
		return null;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Integer a[]={5,32,39,62,58,26,52,43,81};
		System.out.println(getSorted(a));
		
	}

}
