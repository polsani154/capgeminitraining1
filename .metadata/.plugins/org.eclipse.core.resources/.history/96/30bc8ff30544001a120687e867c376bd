	public void Y()  throws InterruptedException
	{
			printpattern(0b100000001);
			wait();
			printpattern(0b001000100);
			wait();
			printpattern(0b000010000);
			wait();
			printpattern(0b000010000);
			wait();
			printpattern(0b000010000);
	}
	public void Z()  throws InterruptedException
	{
		printpattern(0b11111111);
		wait();
		printpattern(0b00000110);
		wait();
		printpattern(0b00011000);
		wait();
		printpattern(0b01100000);
		wait();
		printpattern(0b11111111);
	}
	public static void main(String []args) throws InterruptedException
	{
		pattern p=new pattern();
//		p.A();
//		p.B();
		p.Z();

	}

