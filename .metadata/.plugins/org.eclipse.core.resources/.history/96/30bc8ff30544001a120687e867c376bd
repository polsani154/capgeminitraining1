            printpattern(0b100000001);
            wait();
            printpattern(0b100001101);
            wait();
            printpattern(0b100000110);
            wait();
			printpattern(0b011111001);
	}
	public void R()  throws InterruptedException
	{
            printpattern(0b111111100);
            wait();
            printpattern(0b100000010);
            wait();
            printpattern(0b101111100);
            wait();
            printpattern(0b100011000);
            wait();
			printpattern(0b100000110);
	}
	public void S()  throws InterruptedException
	{
			printpattern(0b011111111);
			wait();
            printpattern(0b100000000);
            wait();
            printpattern(0b011111110);
            wait();
            printpattern(0b000000001);
            wait();
			printpattern(0b111111110);
	}	
	public void T()  throws InterruptedException
	{
            printpattern(0b111111111);
